#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- \
		--enable-gtk-doc

override_dh_auto_build:
	dh_auto_build
	make -C po udisks.pot

override_dh_auto_test:
	# don't run test suite on buildds

override_dh_makeshlibs:
	dh_makeshlibs -Xusr/lib/polkit-1/

override_dh_auto_install:
	dh_auto_install
