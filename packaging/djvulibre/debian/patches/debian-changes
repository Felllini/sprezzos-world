 }
 
 
+int
+check_eachpage(const char *s)
+{
+  const char *p = s;
+  int hasd = 0;
+  int size = 0;
+  char c;
+  while ((c = *s++))
+    if (c == '%')
+      {
+        c = *s++;
+        if (c == '%')
+          continue;
+        if (hasd)
+          return 0;
+        if (c == '-' || c == '+' || c == ' ')
+          c = *s++;
+        while (c >= '0' && c <= '9')
+          {
+            size = 10 * size + c - '0';
+            c = *s++;
+          }
+        if (c != 'd')
+          return 0;
+        hasd = 1;
+      }
+  if (size == 0)
+    size = 30;
+  if (hasd == 1 && size>0 && size<1000)
+    return size + s - p;
+  return 0;
+}
+
 
