#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

override_dh_auto_install:
	find debian/tmp -iname \*.la -delete
	dh_auto_install --

override_dh_auto_configure:
	dh_auto_configure -- \
		--libdir=\$${prefix}/lib/$(DEB_HOST_MULTIARCH)

%:
	dh $@  --with autotools-dev
